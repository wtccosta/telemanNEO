DROP TABLE IF EXISTS `chamado`;

CREATE TABLE `chamado` (
  `id` bigint NOT NULL AUTO_INCREMENT,
  `solicitante` varchar(60) NOT NULL,
  `endereco` varchar(90) NOT NULL,
  `tecnico_responsavel_id` bigint DEFAULT NULL,
  `local_id` bigint DEFAULT NULL,
  `telefone_contato` varchar(15) NOT NULL,
  `email` varchar(45) DEFAULT NULL,
  `descricao` varchar(255) NOT NULL,
  `solucao` varchar(180) DEFAULT NULL,
  `abertura` datetime NOT NULL,
  `alteracao` datetime DEFAULT NULL,
  `fechamento` datetime DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3;





