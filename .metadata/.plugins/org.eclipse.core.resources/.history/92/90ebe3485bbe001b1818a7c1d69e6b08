DROP TABLE IF EXISTS `local`;

CREATE TABLE `local` (
  `id` bigint NOT NULL AUTO_INCREMENT,
  `nome` varchar(255) DEFAULT NULL,
  `secretaria_id` bigint DEFAULT NULL,
  `endereco_id` bigint DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `fk_local_secretaria_id` (`secretaria_id`),
  KEY `fk_local_endereco_id` (`endereco_id`),
  CONSTRAINT `fk_local_endereco_id` FOREIGN KEY (`endereco_id`) REFERENCES `endereco` (`id`),
  CONSTRAINT `fk_local_secretaria_id` FOREIGN KEY (`secretaria_id`) REFERENCES `secretaria` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=53515 DEFAULT CHARSET=utf8mb3;






