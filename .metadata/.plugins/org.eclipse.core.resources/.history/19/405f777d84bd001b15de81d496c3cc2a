<!DOCTYPE html>
<html>

<head th:replace="~{base :: head('GestÃ£o de Agrupadores')}"></head>

<body>
	<nav th:replace="~{base :: menu}"></nav>
	<div class="container" id="app">
		{{message}}

	</div>
	
	<script src="https://cdn.jsdelivr.net/npm/vue@2"></script>
	<script>
		const app = new Vue({
			el: '#app',
			data: {
				message: 'Hello Vue!'
			},
			methods: {
				loadData: () => {
					this.message = this.message.split('').reverse().join('')
				}
			},
			mounted: () => {
				this.loadData();
			}
		})
	</script>



</body>

